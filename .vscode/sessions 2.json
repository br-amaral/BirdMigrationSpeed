                "commands": [
                    "git status",
                    ""
                ]
            },
            {
                "name": "R",
                "autoExecuteCommands": false,
                "icon": "run",
                "color": "terminal.ansiBrightCyan",
                "commands": [
                    "R"
                ]
            }
        ]
    }
}